let g:spf13_keep_trailing_whitespace = 1
color peaksea
"let g:syntastic_ignore_files = ['\.py$', 'semanage']
let g:syntastic_ignore_files = ['\.py$']
set wrap
let &colorcolumn=join(range(81,999),",")
set spell " spell check

" airline options"
let g:airline#extensions#whitespace#enabled = 0 " it's called a statusline"
